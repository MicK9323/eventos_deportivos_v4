<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pit">
	<resultMap type="AdminIbatis" id="resultAdminIbatis">
		<id property="dni" column="dni"/>
		<result property="password" column="pass"/>
		<result property="fecRegistro" column="fec_registro"/>
		<result property="estado" column="estado"/>
	</resultMap>
	<select id="iniciarSesion" statementType="CALLABLE" resultMap="resultAdminIbatis">
		call sp_IniciarSesion( #{dni}, #{password} )
	</select>
</mapper>








