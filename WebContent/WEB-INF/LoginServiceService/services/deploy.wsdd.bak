<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from LoginServiceService WSDL service -->

  <service name="LoginService" provider="java:RPC" style="document" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://services"/>
      <parameter name="wsdlServiceElement" value="LoginServiceService"/>
      <parameter name="schemaQualified" value="http://beans,http://services"/>
      <parameter name="wsdlServicePort" value="LoginService"/>
      <parameter name="className" value="services.LoginServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="LoginService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="loginJugador" qname="loginJugador" returnQName="retNS:loginJugadorReturn" xmlns:retNS="http://services" returnType="rtns:JugadorDTO" xmlns:rtns="http://beans" soapAction="" >
        <parameter qname="pns:dni" xmlns:pns="http://services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:clave" xmlns:pns="http://services" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="loginJugador"/>

      <typeMapping
        xmlns:ns="http://beans"
        qname="ns:JugadorDTO"
        type="java:beans.JugadorDTO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
